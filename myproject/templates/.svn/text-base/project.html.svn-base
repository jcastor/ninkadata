{% extends "index.html" %}


{% block content %}
<a href="{%url ninkadata.views.mainview%}">Home</a> > <a href="{% url ninkadata.views.mainview project.distribution %}">{{project.distribution}}</a> > <a href="{% url ninkadata.views.project project.distribution project.id %}"> {{project.name}} </a>
<p class="title">version listing - {{ project.name }}</p>
	<table>
		<tr>
			<th>Version</th>
			<th>Filecount</th>
			<th>Distribution</th>
		</tr>
		{% for projects in projectWithName %}
		<tr>
			<td><a href="{% url ninkadata.views.files projects.id '1' %}" class="projectname">{{ projects.version }}</a></td>
			<td>{{projects.count}}</td>
			<td>{{projects.distribution}}</td>
		</tr>
		{% endfor %}
	</table>
{% endblock %}
